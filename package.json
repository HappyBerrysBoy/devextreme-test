    "vue-router": "^4.0.1",
    "vue3-popper": "^1.3.0",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "devextreme-cli": "1.2.21",
    "devextreme-themebuilder": "^21.1.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
